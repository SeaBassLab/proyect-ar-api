{
    "name": "proyect-ar",
    "version": "21.3.3",
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "env": {
      "DB_USER": "db-user",
      "DB_PASSWORD": "db-password",
      "DB_HOST": "db-host",
      "DB_NAME": "db-name"
    }
  }